{
    "manifest_version": 3,
    "name": "hangaround",
    "short_name": "hangaround",
    "description": "Know which website you are mostly hanging around!",
    "version": "1.0.0",
    "author": "plsankar1996@gmail.com",
    "action": {
        "default_popup": "popup/popup.html",
        "default_title": "hangaround"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "contentscript.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "storage"
    ]
}