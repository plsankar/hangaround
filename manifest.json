{
    "manifest_version": 2,
    "name": "hangaround",
    "description": "hangaround chrome extension",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "app/popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "backend/contentscript.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}